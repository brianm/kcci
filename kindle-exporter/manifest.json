{
  "manifest_version": 3,
  "name": "Kindle Library Exporter",
  "version": "1.0",
  "description": "Export your Kindle library as JSON",
  "permissions": ["activeTab", "scripting", "downloads"],
  "host_permissions": ["https://read.amazon.com/*"],
  "action": {
    "default_popup": "popup.html"
  }
}
